Opacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.8\" condition=\"overlay && bg.type !== 'color'\">\n    </mbr-parameters>\n\n    <div class=\"mbr-overlay\" mbr-if=\"overlay && bg.type !== 'color'\" opacity=\"{{overlayOpacity}}\" bg-color=\"{{overlayColor}}\"></div>\n\n    <div class=\"pos_rel container\">\n        <div class=\"main_section_title\" mbr-if=\"showSubtitle || showTitle\">\n            <div class=\"empty_content\"></div>\n            <div class=\"main_title\">\n                <h3 class=\"mbr-section-subtitle mbr-semibold mbr-fonts-style\" mbr-theme-style=\"display-7\" mbr-if=\"showSubtitle\" data-app-selector=\".mbr-section-subtitle\">UI/UX</h3>\n                <h1 class=\"mbr-section-title mbr-fonts-style mb-0\" data-app-selector=\".mbr-section-title\" mbr-theme-style=\"display-2\" mbr-if=\"showTitle\"><b>Chinese Property Searching App Revamp</b></h1>\n            </div>\n        </div>\n        <div class=\"block-content\">\n                <div class=\"text-wrapper text-white\">\n                    <h4 class=\"card_subtitle mbr-medium mb-2 mbr-fonts-style\" mbr-theme-style=\"display-2\" mbr-if=\"cardSubtitle\" data-app-selector=\".card_subtitle\"><b>Overview</b></h4>\n                    <h3 class=\"card_title mbr-fonts-style mb-3\" data-app-selector=\".card_title\" mbr-theme-style=\"display-7\" mbr-if=\"cardTitle\">Juwai.com is an online property searching platform for Chinese real estate buyers to explore oversea properties. This project aims to improve the overall performance of the existing mobile app, to optimise user experience, increase the traffic and drive conversion.</h3>\n                    <p class=\"mbr-text mbr-fonts-style\" mbr-theme-style=\"display-7\" mbr-if=\"showText\" data-app-selector=\".mbr-text, .mbr-section-btn\"><i>Role:&nbsp;95% UX Research，100% UI Design</i></p>\n                    <div class=\"mbr-section-btn\" mbr-if=\"showButtons\" mbr-buttons mbr-theme-style=\"display-4\"><a class=\"btn btn-white-outline\" href=